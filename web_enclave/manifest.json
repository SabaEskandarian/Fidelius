{
  "name": "Tag Parser",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Analyzes a web page's script and form tags, then writes them to a file",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {},
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "<all_urls>",
    "tabs"
  ]
}
